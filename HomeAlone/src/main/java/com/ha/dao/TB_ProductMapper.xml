<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ha.dao.TB_ProductDAO">


	<select id="select" resultType="com.ha.entity.TB_Product">
	
		select * from tb_product
	
	</select>
	
	
	<select id="prodName" resultType="com.ha.entity.TB_Product">
	
		select * from tb_product
		where prd_seq = #{prod_seq}
		
	
	
	
	</select>




















</mapper>