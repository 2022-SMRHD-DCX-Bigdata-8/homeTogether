<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ha.dao.TB_BasketMapper">

	
	<insert id="insert" parameterType="com.ha.entity.TB_Basket">
	
		insert into tb_basket
		(
		 nick,
		 prod_seq,
		 basket_cnt
		 
		)value(
		 #{nick},
		 #{pNumber},
		 #{pCnt} 
		
		)
		
	</insert>










</mapper>